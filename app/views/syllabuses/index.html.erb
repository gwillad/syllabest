<h1> Displaying All Syllabi </h1>
<br><%= link_to 'New', new_user_syllabus_path =%></br>
<table>
<tr>
<th>Title</th>
<th>Course No.</th>
<th>Section No.</th>
<th>Term</th>
</tr>
<% @syllabuses.each do |syllabus| %>
<tr>
<td><%= syllabus.title %></td>
<td><%= syllabus.course_num %></td>
<td><%= syllabus.section_num %></td>
<td><%= link_to 'View', syllabus_path(syllabus) %></td>
</tr>
<% end %>
</table>
<br>

