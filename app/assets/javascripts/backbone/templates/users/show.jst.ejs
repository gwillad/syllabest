					<th>First Name</th>
					<th>Last Name</th>
					<th>School</th>
					<th>Office</th>
					<th></th>
				</tr>
				<tr id="user_profile">
					<td><%= user.get("first_name") %></td>
					<td><%= user.get("last_name") %></td>
					<td><%= user.get("school") %></td>
					<td><%= user.get("office") %></td>
				</tr>
			</table>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<button type="button" class="btn btn-default" id="add_syllabus">Add Syllabus</button>
		</div>
